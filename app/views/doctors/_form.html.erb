<%= form_for doctor do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <br><br>

  <%= f.label :contact_no, "Contact No" %>
  <%= f.number_field :contact_no %>
  <br><br>

  <%= f.label :speciality, "Speciality" %>
  <%= f.text_field :speciality %>
  <br><br>
  <%= f.label :from_time, "From" %>
  <%= f.time_field :from_time %>
  <br><br>
  <%= f.label :to_time, 'TO' %>
  <%= f.time_field :to_time %>
  <br><br>
  <%= f.fields_for :profile, doctor.profile do |profile_fields| %>
    <%= profile_fields.label :dob, 'DOB' %>
    <%= profile_fields.date_field :dob %>
  <% end %>

  <%= f.submit %>
<% end %>