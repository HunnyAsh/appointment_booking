<%= form_for doctor do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <br><br>

  <%= f.label :contact_no, "Contact No" %>
  <%= f.number_field :contact_no %>
  <br><br>

  <%= f.label :speciality, "Speciality" %>
  <%= f.text_field :speciality %>
  <br><br>

  <%= f.submit %>
<% end %>